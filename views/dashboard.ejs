   <style>
    #weatherWidget .currentDesc {
        color: #ffffff!important;
    }
        .traffic-chart {
            min-height: 335px;
        }
        #flotPie1  {
            height: 150px;
        }
        #flotPie1 td {
            padding:3px;
        }
        #flotPie1 table {
            top: 20px!important;
            right: 0px!important;
        }

        #flotPie2  {
            height: 150px;
        }
        #flotPie2 td {
            padding:3px;
        }
        #flotPie2 table {
            top: 20px!important;
            right: 0px!important;
        }

        .chart-container {
            display: table;
            min-width: 450px ;
            text-align: left;
            padding-top: 10px;
            padding-bottom: 10px;
        }
        #flotLine5  {
             height: 105px;
        }

        #flotBarChart {
            height: 150px;
        }
        #cellPaiChart{
            height: 160px;
        }

